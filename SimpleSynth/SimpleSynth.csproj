<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.3.0</Version>
    <Authors>Connor14</Authors>
    <Company>Connor14</Company>
    <PackageProjectUrl>https://github.com/Connor14/SimpleSynth</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>A simple C# library that can read a MIDI file and spit out a WAV file.</Description>
    <Copyright>2019 Connor Schmidt</Copyright>
    <PackageTags>music, midi, wav, synth, synthesizer</PackageTags>
    <PackageReleaseNotes>Improve performance by reusing note signals of same Channel, Note, and Tick Count.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MidiSharp" Version="0.9.6516.2348" />
    <PackageReference Include="NWaves" Version="0.9.1" />
  </ItemGroup>

</Project>
